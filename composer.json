{
    "name": "bearcodi/docker-secrets-exposer",
    "description": "Docker secrets expander for Laravel environment configuration.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Babič",
            "email": "mark@bearcodi.com"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.4.0",
        "phpunit/phpunit": "~5.7.0"
    },
    "autoload": {
        "psr-4": {
            "Bearcodi\\DockerSecrets\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bearcodi\\DockerSecrets\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}